                 title="Authorization Event Activate Notification Initial/Complete Vision Care">
   <mif:businessName name="Auth Init Results Init/Comp, VisionCare"/>
   <mif:packageLocation root="DEFN" section="AM" subSection="FI" domain="CR" artifact="IN"
                        realmNamespace="UV"
                        version="03"
