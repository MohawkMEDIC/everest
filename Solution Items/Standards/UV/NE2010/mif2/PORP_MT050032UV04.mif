      &lt;excerpt&gt;
         &lt;highlight&gt;
   	 &lt;substanceAdministration&gt;
   	    ...
   	    &lt;consumable&gt;
   	       &lt;activeIngredient&gt;
                    &lt;id root="60a4db80-573d-11db-b0de-0800200c9a66"/&gt;
   	       &lt;/activeIngredient&gt;
   	    &lt;/consumable&gt;
   	    ...
   	 &lt;/substanceAdministration&gt;
         &lt;/highlight&gt;
      &lt;/excerpt&gt;
   &lt;/section&gt;
			   </mif:pre>
