								<denominator xsi:type="PQ" flavorId="PQ.TIME" value="1" unit="d"/>
							</frequency>
						</effectiveTime>
					</data>
					<businessName name="3 times per day"/>
				</staticExample>
			</appInfo>
		</annotations>
