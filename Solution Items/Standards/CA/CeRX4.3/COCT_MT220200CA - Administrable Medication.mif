                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">C7</html:p>
                     </mif:text>
                  </mif:mapping>
               </mif:appInfo>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="ManufacturedMaterial"
