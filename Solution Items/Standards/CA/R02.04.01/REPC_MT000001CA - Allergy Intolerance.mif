   <mif:containedClass>
      <mif:class graphicLinkId="7ADA2640-AF82-0912-9B7B-8226DB218910" name="Records"
                 isAbstract="true">
         <mif:derivedFrom staticModelDerivationId="1" className="Act"/>
         <mif:childClass graphicLinkId="CE62FB7A-6F55-809E-FD19-3AB72B94DAD5" sortKey="001"
                         name="CausalityAssessment"/>
         <mif:childClass graphicLinkId="1BEDB3E4-53E0-B9C3-F5F5-FA4658D2C459" sortKey="002"
                         name="AllergyTestEvent"/>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="E589ADF9-6092-ECCA-E2A6-657DCF6C4352" name="SeverityObservation"
                 isAbstract="false">
         <mif:businessName name="Allergy/Intolerance Severity Level"/>
         <mif:derivedFrom staticModelDerivationId="1" className="Observation"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">This is a ranking/ an assertion of the seriousness of the diagnosed/reported medical condition.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Allows providers to determine appropriate managements for contraindications involving such medical conditions.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
