      <![CDATA[
         <li>
          <a href="~/$Url$.html" title="$Abstract$">$Title$</a> $Abstract$
				</li>
			]]>
    </propertyTemplate>
    <propertyTemplate name="TocTitle" property="Children">
      <![CDATA[
			<h2>Defined Items</h2>
      <p>The following items are defined in this article</p>
				<ul>
				]]>
    </propertyTemplate>
    <propertyTemplate name="TocFooter" property="Children">
      <![CDATA[</ul>]]>
    </propertyTemplate>

    <![CDATA[
      <div style="border:solid 1px #ddd; float:right; width:20em; padding:2px 2px 2px 2px; float:right">
        <div>
          <b>Navigation</b>
          {{ wiki.toc{path: "~/$Url$"} }}
        </div>
        <div style="background-color:#eee; border:solid 1px #ddd; width:100%">
