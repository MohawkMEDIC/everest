      only original text
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'codeDef']">
      If not null, the element must be coded and must be drawn from the set of codes determined
      but the identified domain and the realm in which the instance is constructed. The set of code
      is fixed to those values in place at the time this model was successfully balloted
    </xmlElementTemplate>

    <!-- Conformance -->
    <xmlElementTemplate match="//*[local-name() = 'conformDef'][@value='Mandatory']">
      Value must always be supplied (ie: No Nulls)
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'conformDef'][@value='Required']">
      All implementors must support this property. They must be able to transmit, or to receive and
      usefully handle the concept
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'conformDef'][@value='Optional']">
      the concept is optional. Implementers must declare whether they support the concept or not.
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'conformDef'][@value='Populated']">
      Implementers must support and must always send a value, however a null flavor is permitted as a value
    </xmlElementTemplate>

    <!-- Multiplicity -->
    <xmlElementTemplate match="//*[local-name() = 'multiplicityDef'][@value = '*']">
      Multiplicity is unbounded, any number of repetitions is permitted
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'multiplicityDef'][@value = '1']">
      Does not repeat
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'multiplicityDef']">
      Repeats up to $value$ times.
    </xmlElementTemplate>

    <!-- Update Modes -->
    <xmlElementTemplate match="//*[local-name() = 'updateDef'][@value = 'Unknown']">
      Update mode not specified
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'updateDef'][@value = 'Add']">
      New value is appended to the existing data set
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'updateDef'][@value = 'Replace']">
      New value will replace an existing value
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'updateDef'][@value = 'Delete']">
      Existing value is removed
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'updateDef'][@value = 'NoChange']">
      No change is permitted
    </xmlElementTemplate>

    <!-- Property Types -->
    <xmlElementTemplate match="//*[local-name() = 'propDef'][@value = 'Structural']">
      This attribute is structural in nature (example: in XML, this would equate to an attribute)
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'propDef'][@value = 'NonStructural']">
      This attribute is non-structural in nature (example: in XML, this would equate to an element)
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'propDef'][@value = 'TraversableAssociation']">
      This attribute represents a Traversable association to another class
    </xmlElementTemplate>
    <xmlElementTemplate match="//*[local-name() = 'propDef'][@value = 'NonTraversableAssociation']">
      This attribute represents a non-traversable association to another class. These are usually not
      rendered in the ITS
    </xmlElementTemplate>
    <![CDATA[
      <h4>$Name$ $BusinessName$</h4>
      $TraversalHeader$
      @AlternateTraversalNames@
      $TraversalFooter$
      <table style="width:80%; margin-left:10px; border:solid 1px #000; margin-bottom:5px" cellspacing="0" cellpadding="0" border="0">
        <tr>
          <td valign="top" style="padding:2px 2px 2px 2px; font-weight:bold; border-right:solid 1px #000;; border-bottom:solid 1px #000; color:navy; background-color:#ddd;">Datatype</td>
          <td align="center" style="padding:2px 2px 2px 2px; border-right:solid 1px #000; border-bottom:solid 1px #000">$Type$</td>
          <td style="padding:2px 2px 2px 2px; border-bottom:solid 1px #000">See Documentation</td>
        </tr>
        $FixedValue$
        $DefaultValue$
        <tr>
          <td valign="top" style="padding:2px 2px 2px 2px; font-weight:bold; border-right:solid 1px #000; color:navy; background-color:#ddd; border-bottom:solid 1px #000">Type</td>
          <td align="center" style="padding:2px 2px 2px 2px; border-right:solid 1px #000; border-bottom:solid 1px #000">$PropertyType$</td>
          <td style="padding:2px 2px 2px 2px; border-bottom:solid 1px #000"><propDef value="$PropertyType$"/></td>
        </tr>
        <tr>
          <td valign="top" style="padding:2px 2px 2px 2px; font-weight:bold; border-right:solid 1px #000; color:navy; background-color:#ddd; border-bottom:solid 1px #000">Conformance</td>
          <td align="center" style="padding:2px 2px 2px 2px; border-right:solid 1px #000; border-bottom:solid 1px #000">$Conformance$</td>
          <td style="padding:2px 2px 2px 2px; border-bottom:solid 1px #000"><conformDef value="$Conformance$"/></td>
        </tr>
        <tr>
          <td valign="top" style="padding:2px 2px 2px 2px; border-bottom:solid 1px #000; font-weight:bold; border-right:solid 1px #000; color:navy; background-color:#ddd;">Multiplicity</td>
          <td align="center" style="padding:2px 2px 2px 2px; border-right:solid 1px #000; border-bottom:solid 1px #000">$MaxOccurs$</td>
          <td style="padding:2px 2px 2px 2px; border-bottom:solid 1px #000"><multiplicityDef value="$MaxOccurs$"/></td>
        </tr>
        $CodeStrength$
        $CodeDomain$
        $UpdateMode$
        @Validation@
      </table>
      <table style="width:80%; margin-left:10px; border:solid 1px #000;" cellspacing="0" cellpadding="0" border="0">
        <tr>
          <td valign="top" rowspan="2" style="padding:2px 2px 2px 2px; font-weight:bold; border-right:solid 1px #000; color:navy; background-color:#ddd; border-bottom:solid 1px #000">Description</td>
          <td valign="top" style="padding:2px 2px 2px 2px;">$Documentation$</td>
        </tr>
        $TypeDoc$
        <tr>
          <td valign="top" style="padding:2px 2px 2px 2px; border-right:solid 1px #000; font-weight:bold; border-right:solid 1px #000; color:navy; background-color:#ddd;">Rationale</td>
          <td style="padding:2px 2px 2px 2px; border-right:solid 1px #000;">$Rationale$</td>
        </tr>
        @OtherDocumentation@
        $StateMachine$
      </table>
    ]]>

  </featureTemplate>

  <!-- Code Combine Annotation -->
  <featureTemplate feature="MohawkCollege.EHR.gpmr.COR.CodeCombineAnnotation"
                   createNewPage="false">
    <propertyTemplate propertyName="OriginalType">$value$</propertyTemplate>
    <![CDATA[
      <b>Identical to:</b> $OriginalType$
    ]]>
  </featureTemplate>

  <!-- Class Feature Template -->
  <featureTemplate feature="MohawkCollege.EHR.gpmr.COR.Class"
                   createNewPage="true" pageTitleProperty="PageTitle" pageAbstractProperty="PageAbstract" pageParent="PageParent">
    <propertyTemplate name="PageTitle">$Name$</propertyTemplate>
    <propertyTemplate name="PageAbstract">(Class) $BusinessName$</propertyTemplate>
    <propertyTemplate name="PageParent">Sub Systems/$ContainerName$</propertyTemplate>
    <propertyTemplate name="Documentation" property="Documentation">$feature$</propertyTemplate>
    <propertyTemplate property="DerivedFrom" escaped="true">$value$</propertyTemplate>
    <propertyTemplate name="Content" property="Content">
      <![CDATA[
        $feature$
      ]]>
    </propertyTemplate>
    <propertyTemplate property="BaseClass" name="Extension">
      <![CDATA[
      <b>Specializes</b><ul><li>$feature$</li></ul>
      ]]>
    </propertyTemplate>
    <propertyTemplate property="SpecializedBy" name="GeneralizationTitle">
      <![CDATA[
        <b>Specialized By</b><ul>
      ]]>
    </propertyTemplate>
    <propertyTemplate property="Realizations" name="ProjectionTitle">
      <![CDATA[<b>Realization Of</b><ul>]]>
    </propertyTemplate>
    <propertyTemplate property="BusinessName">
      <![CDATA[<b>Business Name</b> $value$<br/>]]>
    </propertyTemplate>
    <propertyTemplate property="Realizations" name="ProjectionFoot">
      <![CDATA[</ul>]]>
    </propertyTemplate>
    <propertyTemplate property="Realizations">
      <![CDATA[<li>$feature$</li>]]>
    </propertyTemplate>
    <propertyTemplate property="SpecializedBy" name="GeneralizationFoot">
      <![CDATA[</ul>]]>
    </propertyTemplate>
    <propertyTemplate property="SpecializedBy">
      <![CDATA[<li>$feature$</li>]]>
    </propertyTemplate>
    <propertyTemplate name="TypeParameterHead" property="TypeParameters">
      <![CDATA[<b>Type Parameters (Generics)</b><ul>]]>
    </propertyTemplate>
    <propertyTemplate property="TypeParameters">
      <![CDATA[<li>$ParameterName$</li>]]>
    </propertyTemplate>
    <propertyTemplate property="TypeParameters" name="TypeParameterFoot" >
      <![CDATA[</ul>]]>
    </propertyTemplate>
    <propertyTemplate name="AnnotationsHead" property="Annotations">
      <![CDATA[<b>Special Attributes</b><ul>]]>
    </propertyTemplate>
    <propertyTemplate property="Annotations">
      <![CDATA[<li>$feature$</li>]]>
    </propertyTemplate>
    <propertyTemplate property="Annotations" name="AnnotationsFoot" >
      <![CDATA[</ul>]]>
    </propertyTemplate>

    <propertyTemplate property="GraphicalRepresentation" escape="true">
      <![CDATA[
      <div id="classD">
      {{graphviz.dot('$value$')}}
      </div>
      <script type="text/javascript">
        var img;
        for(i = 0; i < document.getElementById("classD").childNodes.length; i++)
          if(document.getElementById("classD").childNodes[i].nodeName == "IMG")
          {
            img = document.getElementById("classD").childNodes[i];
            var a = document.createElement('a');
            a.setAttribute("href", img.src);
            img.setAttribute("border","0");
            img.width=800;
            a.appendChild(img);
            document.getElementById("classD").appendChild(a);
          }
      </script>
      ]]>
    </propertyTemplate>
    <propertyTemplate property="Documentation" name="DocumentationTitle">
      <![CDATA[<h2>Documentation</h2>]]>
    </propertyTemplate>

    <![CDATA[
    
      <blockquote>
      $BusinessName$
      $Extension$
      $ProjectionTitle$
      @Realizations@
      $ProjectionFoot$
      $GeneralizationTitle$
      @SpecializedBy@
      $GeneralizationFoot$
      $TypeParameterHead$
      @TypeParameters@
      $TypeParameterFoot$
      $AnnotationsHead$
      @Annotations@
      $AnnotationsFoot$
      </blockquote>
      
      $DocumentationTitle$
      $Documentation$
      
      <h2>Content</h2>
      
      @Content@
      
			<h2>Definition</h2>
      <h3>Class Diagram</h3>
      $GraphicalRepresentation$


    ]]>
  </featureTemplate>

