               </mif:annotations>
            </mif:subState>
            <mif:subState name="held" parentStateName="normal">
               <mif:annotations>
                  <mif:documentation>
                     <mif:definition>
                        <mif:text>
                           <mif:p>An Act that is still in the preparatory stages has been put aside. No action can occur until the Act is released.</mif:p>
                        </mif:text>
                     </mif:definition>
                  </mif:documentation>
               </mif:annotations>
            </mif:subState>
            <mif:subState name="new" parentStateName="normal">
               <mif:annotations>
                  <mif:documentation>
                     <mif:definition>
                        <mif:text>
                           <mif:p>An Act that is in the preparatory stages and may not yet have been acted upon.</mif:p>
                        </mif:text>
                     </mif:definition>
                  </mif:documentation>
               </mif:annotations>
            </mif:subState>
            <mif:subState name="normal">
               <mif:annotations>
                  <mif:documentation>
                     <mif:definition>
                        <mif:text>
                           <mif:p>Encompasses the expected states of a service object, but excludes "nullified" and "obsolete" which represent unusual terminal states for the life-cycle.</mif:p>
                        </mif:text>
                     </mif:definition>
                  </mif:documentation>
               </mif:annotations>
            </mif:subState>
            <mif:subState name="null"/>
