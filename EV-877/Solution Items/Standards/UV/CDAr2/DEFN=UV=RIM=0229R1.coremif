               <mif:annotations>
                  <mif:documentation>
                     <mif:definition>
                        <mif:text>
                           <mif:p>This Act instance was created in error and has been 'removed' and is treated as though it never existed. A record is retained for audit purposes only.</mif:p>
                        </mif:text>
                     </mif:definition>
                  </mif:documentation>
               </mif:annotations>
            </mif:subState>
            <mif:subState name="obsolete">
               <mif:annotations>
                  <mif:documentation>
                     <mif:definition>
                        <mif:text>
                           <mif:p>This Act instance has been replaced by a new instance.</mif:p>
                        </mif:text>
                     </mif:definition>
                  </mif:documentation>
               </mif:annotations>
            </mif:subState>
            <mif:subState name="suspended" parentStateName="normal">
