            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>The kind of information specified in the acknowledgement message.</mif:p>
                     </mif:text>
                  </mif:definition>
                  <mif:otherAnnotation type="examples" name="examples">
                     <mif:text>
                        <mif:p>Error, warning, information.</mif:p>
                     </mif:text>
