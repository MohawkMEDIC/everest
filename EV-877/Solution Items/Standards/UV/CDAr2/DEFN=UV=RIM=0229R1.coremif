         </mif:attribute>
         <mif:attribute name="value" sortKey="006" minimumMultiplicity="0" maximumMultiplicity="*"
                        isMandatory="false"
                        conformance="U">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>A value associated with the key as provided in the AttentionLine.keyWordText attribute.</mif:p>
                     </mif:text>
                  </mif:definition>
               </mif:documentation>
               <mif:appInfo>
                  <mif:formalConstraint>
                     <mif:text>
                        <mif:p>The data type of the attribute SHALL be constrained to one of the following data types: BL, CV, II, URL, INT, REAL, TS, PQ, MO, IVL&lt;TS&gt;.</mif:p>
