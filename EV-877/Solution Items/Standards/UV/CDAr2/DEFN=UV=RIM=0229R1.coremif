                        isMandatory="false"
                        conformance="U">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>The type of acknowledgement, from an enumerated set of acknowledgement types.</mif:p>
                     </mif:text>
                  </mif:definition>
                  <mif:designComments>
                     <mif:text>
                        <mif:p>Original examples seem to indicate text, not code, by including specific attributes, dates. New examples supplied from concept domain.</mif:p>
                     </mif:text>
                  </mif:designComments>
                  <mif:otherAnnotation type="examples" name="examples">
                     <mif:text>
                        <mif:p>Required attribute missing; unsupported interaction; invalid code system in CNE.</mif:p>
                     </mif:text>
                  </mif:otherAnnotation>
               </mif:documentation>
            </mif:annotations>
            <mif:type name="CE"/>
            <mif:vocabulary>
               <mif:conceptDomain name="AcknowledgementDetailCode"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="text" sortKey="007" minimumMultiplicity="0" maximumMultiplicity="1"
                        isMandatory="false"
                        conformance="U">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>Additional diagnostic information relevant to the message.</mif:p>
                     </mif:text>
                  </mif:definition>
                  <mif:usageNotes>
                     <mif:text>
                        <mif:p>This may be free text or structured data (e.g., XML).</mif:p>
                     </mif:text>
                  </mif:usageNotes>
                  <mif:otherAnnotation type="examples" name="examples">
                     <mif:text>
                        <mif:p>Java exception, memory dump, internal error code, call-stack information</mif:p>
                     </mif:text>
                  </mif:otherAnnotation>
               </mif:documentation>
            </mif:annotations>
            <mif:type name="ED"/>
         </mif:attribute>
         <mif:attribute name="location" sortKey="008" minimumMultiplicity="0" maximumMultiplicity="*"
                        isMandatory="false"
                        conformance="U">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>Definition: The position within the message being acknowledged that is related to the acknowledgement message.</mif:p>
                     </mif:text>
                  </mif:definition>
                  <mif:usageNotes>
                     <mif:text>
                        <mif:p>Only messages with localized errors will have this attribute populated. Open Issue: The specific format for the string that defines the message location needs to be identified. This might be "XPath" or possibly "OCL".</mif:p>
                     </mif:text>
                  </mif:usageNotes>
                  <mif:otherAnnotation type="examples" name="examples">
                     <mif:text>
                        <mif:p>Location of missing required attribute; location of invalid code in CNE; location not valued for unsupported interaction.</mif:p>
                     </mif:text>
                  </mif:otherAnnotation>
               </mif:documentation>
            </mif:annotations>
            <mif:type name="SET">
               <mif:argumentDatatype name="ST"/>
            </mif:type>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
