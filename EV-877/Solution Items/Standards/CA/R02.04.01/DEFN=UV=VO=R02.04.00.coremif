  <valueSet id="2.16.840.1.113883.1.11.20268" name="ActMoodEventCriterion">
    <annotations>
      <documentation>
        <description>
          <text>
            <p xmlns="http://www.w3.org/1999/xhtml">A criterion or condition over service events that must apply for an associated service to be considered.</p>
          </text>
