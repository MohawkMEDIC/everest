      <concept isSelectable="false">
        <annotations>
          <documentation>
            <definition>
              <text>
                <p xmlns="http://www.w3.org/1999/xhtml">The association adds to the existing context associated with the Act.  Both this association and any associations propagated from ancestor Acts are interpreted as being related to this Act.</p>
              </text>
            </definition>
          </documentation>
        </annotations>
        <conceptProperty name="internalId" value="21053"/>
        <printName language="en" preferredForLanguage="true" text="ContextControlAdditive"/>
        <code code="_ContextControlAdditive" status="active"/>
      </concept>
      <concept isSelectable="false">
        <annotations>
          <documentation>
            <definition>
              <text>
