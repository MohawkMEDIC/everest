        <conceptRelationship relationshipName="Specializes">
          <targetConcept code="_PersonNamePartChangeQualifier"/>
        </conceptRelationship>
        <conceptProperty name="internalId" value="10665"/>
        <printName language="en" preferredForLanguage="true" text="spouse"/>
        <code code="SP" status="active"/>
