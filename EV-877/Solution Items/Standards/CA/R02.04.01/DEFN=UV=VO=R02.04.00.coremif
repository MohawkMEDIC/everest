        <conceptRelationship relationshipName="Specializes">
          <targetConcept code="_PersonNamePartAffixTypes"/>
        </conceptRelationship>
        <conceptProperty name="internalId" value="10669"/>
        <printName language="en" preferredForLanguage="true" text="professional"/>
        <code code="PR" status="active"/>
      </concept>
      <concept>
        <annotations>
          <documentation>
            <definition>
              <text>
                <p xmlns="http://www.w3.org/1999/xhtml">The name assumed from the partner in a marital relationship (hence the "SP"). Usually the spouse's family name. Note that no inference about gender can be made from the existence of spouse names.</p>
              </text>
            </definition>
          </documentation>
        </annotations>
        <conceptRelationship relationshipName="Specializes">
          <targetConcept code="_OrganizationNamePartQualifier"/>
