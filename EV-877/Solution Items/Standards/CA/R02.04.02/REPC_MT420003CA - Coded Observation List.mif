   <mif:containedClass>
      <mif:class graphicLinkId="E7595CDE-4933-27DE-4424-6F818570A684" name="ActDefinition"
                 isAbstract="false">
         <mif:derivedFrom staticModelDerivationId="1" className="Act"/>
         <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
