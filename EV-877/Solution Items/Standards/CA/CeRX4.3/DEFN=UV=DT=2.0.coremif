   <mif:datatype name="TS.DATE" title="Date" datatypeKind="Flavor" visibility="public">
      <mif:derivedFrom relationship="restriction">
         <mif:targetDatatype name="TS"/>
      </mif:derivedFrom>
      <mif:annotations>
         <mif:documentation>
            <mif:definition>
               <mif:text>
                  <html:object name="itemName">
                     <html:param name="item" value="datatype"/>
                  </html:object> constrains <html:object name="datatypeRef">
                     <html:param name="datatypeName" value="TS"/>
                  </html:object>  so that it only contains a date value.
					</mif:text>
            </mif:definition>
            <mif:designComments>
               <mif:text>
                  <html:p>
						If the value is not null, there SHALL be 8 or less digits, and there can be no timezone. This flavor is specific
						to the gregorian calendar.
					</html:p>
                  <html:p>
						The string literal representation of this form is YYYY[MM[DD]].
					</html:p>
               </mif:text>
            </mif:designComments>
            <mif:otherAnnotation type="DTDL">
               <mif:data>
flavor Date alias TS.DATE constrains TS;
</mif:data>
            </mif:otherAnnotation>
         </mif:documentation>
         <mif:appInfo>
            <mif:formalConstraint>
               <mif:text/>
               <mif:alternateFormalExpression encoding="HL7DatatypeDefinitionLanguage">
invariant (TS.DATE x) where x.nonNull {
  x.timezone.isNull;
  x.precision.lessOrEqual(8);
  x.calendar.implies(Calendar.GREG);
};</mif:alternateFormalExpression>
            </mif:formalConstraint>
         </mif:appInfo>
